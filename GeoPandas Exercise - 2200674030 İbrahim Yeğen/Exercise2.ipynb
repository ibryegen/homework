   "source": [
    "# YOUR CODE HERE\n",
    "data = pd.read_csv(\"https://raw.githubusercontent.com/GeoTurkey/GMT_COURSES/main/some_posts.csv\")\n",
    "\n",
    "def create_point(row):\n",
    "    point = Point(row['lon'], row['lat'])\n",
    "\n",
    "    return point\n",
    "data['geometry'] = data.apply(create_point, axis=1)\n"
   ]
